{
  "theme": "bootstrap3",
  "startval": {
    "title": "Taichung OSM mapping party 2016",
    "sources": [
      {
        "format": "by-extension",
        "url": "https://raw.githubusercontent.com/g0v/chorusmap/master/data/ex1.geojson",
        "color": "red",
        "icon": "flag"
      },
      {
        "format": "by-extension",
        "url": "https://raw.githubusercontent.com/g0v/chorusmap/master/data/ex3.csv",
        "color": "blue",
        "icon": "bookmark"
      },
      {
        "format": "by-extension",
        "url": "https://raw.githubusercontent.com/g0v/chorusmap/master/data/s142.gpx",
        "color": "green",
        "icon": "star"
      }
    ]
  },
  "schema": {
    "type": "object",
    "title": "configuration",
    "properties": {
      "title": {
        "type": "string"
      },
      "sources": {
        "type": "array",
        "format": "table",
        "items": {
          "type": "object",
          "properties": {
            "format": {
              "type": "string",
              "enum": [
                "by-extension",
                "geojson",
                "gpx",
                "csv"
              ]
            },
            "url": {
              "type": "string",
              "format": "url"
            },
            "color": {
              "type": "string",
              "enum": [
                "blue",
                "cadetblue",
                "darkgreen",
                "darkpurple",
                "darkred",
                "green",
                "orange",
                "purple",
                "red"
              ]
            },
            "icon": {
              "type": "string",
              "enum": [
                "bookmark",
                "flag",
                "glass",
                "home",
                "star"
              ]
            }
          }
        }
      }
    }
  }
}
